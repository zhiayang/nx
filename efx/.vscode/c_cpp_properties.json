{
    "configurations": [
        {
            "name": "efx-windows",
            "includePath": [
                "${workspaceRoot}/include",
                "${workspaceRoot}/../libs/zircon-efi",
                "${workspaceRoot}/../libs/libkrt/include",
                "${workspaceRoot}/../build/sysroot/usr/include"
            ],
            "defines": [
                "__orionx__",
                "_DEBUG"
            ],
            "compilerPath": "${workspaceRoot}/../build/toolchain/bin/x86_64-orionx-g++",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceRoot}/source",
                    "${workspaceRoot}/../libs/libkrt",
                    "${workspaceRoot}/../libs/zircon-efi"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}