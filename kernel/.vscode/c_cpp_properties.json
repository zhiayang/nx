{
    "configurations": [
        {
            "name": "kernel-windows",
            "includePath": [
                "${workspaceFolder:kernel}/include",
                "${workspaceFolder:libkrt}/include",
                "${workspaceFolder:nx}/libs/libm/include",
                "${workspaceFolder:nx}/libs/miniz",
                "${workspaceFolder:nx}/libs",
                "${workspaceFolder:efx}/zircon-efi"
            ],
            "defines": [
                "__orionx__",
                "ORION_KERNEL",
                "__ARCH_x64__",
                "_DEBUG",
                "FUCKIN_WSL_FIX_YOUR_SHIT"
            ],
            "compilerPath": "${workspaceFolder:nx}/build/toolchain/bin/x86_64-orionx-gcc",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder:kernel}",
                    "${workspaceFolder:libkrt}",
                    "${workspaceFolder:nx}/libs/libm"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}
